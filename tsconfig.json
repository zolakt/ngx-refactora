{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@ngx-refactora/auth": [
        "dist/ngx-refactora/auth"
      ],
      "@ngx-refactora/auth/*": [
        "dist/ngx-refactora/auth/*"
      ],
      "@ngx-refactora/auth-provider-auth0": [
        "dist/ngx-refactora/auth-provider-auth0"
      ],
      "@ngx-refactora/auth-provider-auth0/*": [
        "dist/ngx-refactora/auth-provider-auth0/*"
      ],
      "@ngx-refactora/auth-guards": [
        "dist/ngx-refactora/auth-guards"
      ],
      "@ngx-refactora/auth-guards/*": [
        "dist/ngx-refactora/auth-guards/*"
      ],
      "@ngx-refactora/auth-interceptors": [
        "dist/ngx-refactora/auth-interceptors"
      ],
      "@ngx-refactora/auth-interceptors/*": [
        "dist/ngx-refactora/auth-interceptors/*"
      ],
      "@ngx-refactora/auth-provider-oidc": [
        "dist/ngx-refactora/auth-provider-oidc"
      ],
      "@ngx-refactora/auth-provider-oidc/*": [
        "dist/ngx-refactora/auth-provider-oidc/*"
      ],
      "@ngx-refactora/common": [
        "dist/ngx-refactora/common"
      ],
      "@ngx-refactora/common/*": [
        "dist/ngx-refactora/common/*"
      ],
      "@ngx-refactora/cart": [
        "dist/ngx-refactora/cart"
      ],
      "@ngx-refactora/cart/*": [
        "dist/ngx-refactora/cart/*"
      ]
    }
  }
}